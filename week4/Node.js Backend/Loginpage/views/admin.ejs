<!DOCTYPE html>
<html>
<head>
    <title>Admin Panel</title>
    <link rel="stylesheet" href="/css/admin.css">
</head>
<body>
    <h1>Admin - Usernames</h1>
    <table>
        <thead>
            <tr>
                <th>Username</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.username %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
     <div class="button-container">
        <a href="/" class="back-btn">â¬… Go Back</a>
    </div>
</body>
</html>
